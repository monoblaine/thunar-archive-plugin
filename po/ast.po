#. prepare the dialog to query the preferred archiver for the user
#: thunar-archive-plugin/tap-backend.c:79
msgid "Select an archive manager"
msgstr "Escueyi un xestor de ficheros comprimíos"

#: thunar-archive-plugin/tap-backend.c:82
msgid "_Cancel"
msgstr "_Encaboxar"

#: thunar-archive-plugin/tap-backend.c:83
msgid "_OK"
msgstr "_Aceutar"

#. add the header label
#: thunar-archive-plugin/tap-backend.c:101
msgid ""
"Please select your preferred archive manager\n"
"from the list of available applications below:"
msgstr "Por favor, seleiciona el to xestor de ficheros comprimíos preferíu\nde la llista d'aplicaciones disponibles d'embaxu"

#. tell the user that we cannot handle the specified mime types
#: thunar-archive-plugin/tap-backend.c:277
#: thunar-archive-plugin/tap-backend.c:426
msgid "No suitable archive manager found"
msgstr "Nun s'atopó dengún xestor de ficheros comprimíos valederu"

#. execute the action associated with the menu item
#. execute the action
#: thunar-archive-plugin/tap-provider.c:273
#: thunar-archive-plugin/tap-provider.c:329
msgid "Failed to extract files"
msgstr "Fallu al estrayer los ficheros"

#. execute the action associated with the menu item
#: thunar-archive-plugin/tap-provider.c:370
msgid "Failed to create archive"
msgstr "Fallu al crear l'archivu"

#: thunar-archive-plugin/tap-provider.c:424
msgid "Extract _Here"
msgstr "Estrayer _equí"

#: thunar-archive-plugin/tap-provider.c:426
msgid "Extract the selected archive in the current folder"
msgid_plural "Extract the selected archives in the current folder"
msgstr[0] "Estrayer el ficheru esbilláu nel direutoriu d'anguañu"
msgstr[1] "Estrayer los ficheros esbillaos nel direutoriu d'anguañu"

#: thunar-archive-plugin/tap-provider.c:444
msgid "_Extract To..."
msgstr "_Estrayer en..."

#: thunar-archive-plugin/tap-provider.c:446
msgid "Extract the selected archive"
msgid_plural "Extract the selected archives"
msgstr[0] "Estrayer ficheru esbilláu"
msgstr[1] "Estrayer ficheros esbillaos"

#: thunar-archive-plugin/tap-provider.c:464
msgid "Create _Archive..."
msgstr "Facer _archivu comprimíu..."

#: thunar-archive-plugin/tap-provider.c:466
msgid "Create an archive with the selected object"
msgid_plural "Create an archive with the selected objects"
msgstr[0] "Facer un archivu comprimíu col ficheru esbilláu"
msgstr[1] "Facer un archivu comprimíu colos ficheros esbillaos"

#. TRANSLATORS: This is the label of the Drag'n'Drop "Extract here" menu item
#: thunar-archive-plugin/tap-provider.c:532
msgid "_Extract here"
msgstr "_Estrayer equí"

#: thunar-archive-plugin/tap-provider.c:534
msgid "Extract the selected archive here"
